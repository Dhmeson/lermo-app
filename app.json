{
  "expo": {
    "name": "lermo",
    "slug": "lermo",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dhmes.lermo",
      "versionCode": 5
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5636a47b-8c43-4b19-b881-2d2b8b35defb"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 5000,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/5636a47b-8c43-4b19-b881-2d2b8b35defb"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
